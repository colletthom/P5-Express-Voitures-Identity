@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

    <h1>Liste des Roles</h1>
    <a asp-action="CreateRole">Creation role</a>
    <Table class="table table-striped table-bordered">
        <thead>
            <tr>
                <td>ID</td>
                <td>Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                </tr>
            }
        </tbody>
    </Table>
