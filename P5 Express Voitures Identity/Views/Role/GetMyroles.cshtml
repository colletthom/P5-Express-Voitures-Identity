@model IEnumerable<string>

<h1>Liste de mes Roles</h1>
<Table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role</td>
            </tr>
        }
    </tbody>
</Table>